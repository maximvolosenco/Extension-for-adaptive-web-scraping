{

    "manifest_version": 3,
    "name": "Adaptive",
    "description": "Extension for adaptive web scraping",
    "version": "0.0.1",

    "icons": {
        "16": "/assets/selective16.png",
        "32": "/assets/selective32.png",
        "128": "/assets/selective128.png"
    },

    "action": {
        "default_icon": {
            "16": "/assets/selective16.png",
            "32": "/assets/selective32.png",
            "128": "/assets/selective128.png"
        }
    },

    "content_scripts": [
    {
      "matches": ["https://www.google.com/*"],
      "js": ["smth.js"]
    }
  ]
  }
